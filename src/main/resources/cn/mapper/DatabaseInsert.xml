<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.DatabaseInsert">
    <insert id="insertBook"  parameterType="list">
        INSERT INTO products(name ,price,category,prum,imgurl)value
        <foreach collection="list" item="product" separator=",">
        (#{product.name},#{product.price},#{product.category},#{product.prum},#{product.imgurl})
        </foreach>
    </insert>
</mapper>